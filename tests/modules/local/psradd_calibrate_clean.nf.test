    test("J1534-5334_2023-05-05-01:07:10_1: period: slow   DM: low    obs_len: short ") {
        tag "short"
        when {
            params {
                outdir = "$outputDir"
            }
            process {
                """
                input[0] =  Channel.fromList([
                    tuple(
                        // meta map
                        [
                            id: "J1534-5334_2023-05-05-01:07:10_1",
                            pulsar: "J1534-5334",
                            utc: "2023-05-05-01:07:10",
                            beam: 1,
                            dur: 116.53766938317756,
